{
  "firestore": {
    "database": "(default)",
    "location": "nam5",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": {
    "source": "functions",
    "ignore": [
      "node_modules",
      ".git",
      ".runtimeconfig.json",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "scripts/**",
      "serviceAccountKey.json",
      ".env",
      ".env.*"
    ]
  }
}
